{
  "connection": "default",
  "collectionName": "items",
  "info": {
    "name": "item",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "integer"
    },
    "size": {
      "enum": [
        "sm69",
        "sm74",
        "sm79",
        "sm84"
      ],
      "type": "enumeration"
    },
    "category": {
      "model": "categories",
      "via": "items"
    },
    "orders": {
      "collection": "order",
      "via": "items"
    },
    "sex": {
      "enum": [
        "girl",
        "boy"
      ],
      "type": "enumeration"
    },
    "count": {
      "type": "integer"
    },
    "foto": {
      "type": "string"
    }
  }
}